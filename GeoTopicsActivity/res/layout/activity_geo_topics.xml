<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".StartActivity" >
    
    <Button
           android:id="@+id/start_test_es"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentLeft="true"
           android:layout_alignParentTop="true"
           android:layout_marginLeft="78dp"
           android:layout_marginTop="176dp"
           android:text="test_es" />

    <ImageButton
        android:id="@+id/start_bookmark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/start_my_comments"
        android:layout_alignTop="@+id/start_favourite"
        android:background="@null"
        android:padding="20dp"
        android:src="@drawable/ic_action_bookmark" />

    <ImageButton
        android:id="@+id/start_my_comments"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/start_favourite"
        android:layout_alignLeft="@+id/start_test_es"
        android:layout_marginLeft="41dp"
        android:background="@null"
        android:padding="20dp"
        android:src="@drawable/ic_action_my_comments" />

    

    <ImageButton
        android:id="@+id/start_favourite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/start_browse_top_level"
        android:layout_below="@+id/start_browse_top_level"
        android:layout_marginTop="17dp"
        android:background="@null"
        android:padding="20dp"
        android:src="@drawable/ic_action_start_favourite" />

       <ImageButton
           android:id="@+id/start_browse_top_level"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_below="@+id/start_test_es"
           android:layout_marginTop="33dp"
           android:layout_toLeftOf="@+id/start_bookmark"
           android:background="@null"
           android:padding="20dp"
           android:src="@drawable/ic_action_browse" />

       <RelativeLayout
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentTop="true"
           android:layout_marginTop="88dp"
           android:layout_toLeftOf="@+id/start_test_es" >
       </RelativeLayout>

       <GridLayout
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentTop="true"
           android:layout_marginRight="20dp"
           android:layout_marginTop="116dp"
           android:layout_toLeftOf="@+id/start_browse_top_level" >
       </GridLayout>

</RelativeLayout>
